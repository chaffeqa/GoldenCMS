<!--  START Element -->
<section class="admin new-element-select">
  <span class="controls">
    <%= form_tag(new_admin_element_path(:shortcut => @node.shortcut, :page_area => page_area)) do %>
      <%= select_tag :elem_controller, options_for_select(Element.get_elem_select), :prompt => "Add an Element" %>
      <%= submit_tag "Add Element", :class => 'btn add small' %>
    <% end %>
  </span>
</section>
<!-- END Element -->

